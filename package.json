{
  "name": "pro-store",
  "version": "1.0.0-beta",
  "description": "Better localStorage, Subscribe localStorage",
  "main": "./lib/pro-store.cjs.js",
  "module": "./lib/pro-store.esm.js",
  "browser": "./lib/pro-store.min.js",
  "unpkg": "./lib/pro-store.min.js",
  "scripts": {
    "lint": "eslint src --ext .js,.ts",
    "test": "jest --no-cache",
    "build": "rm -rf ./lib/* && rollup -c"
  },
  "keywords": [
    "localStorage",
    "store",
    "subscribe",
    "listen"
  ],
  "author": "GavinBirkhoff",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src*/**/*.ts": [
      "prettier --config .prettierrc.js --write",
      "eslint",
      "git add"
    ],
    "src*/**/*.json": [
      "prettier --config .prettierrc.js --write",
      "eslint",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/GavinBirkhoff/pro-store/issues"
  },
  "homepage": "https://github.com/GavinBirkhoff/pro-store#readme",
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "codecov": "^3.8.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-license": "^2.8.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^28.0.5",
    "tslib": "^1.14.1",
    "typescript": "^4.7.4"
  }
}